{
  "brichka_notebook_import": {
    "prefix": "brni",
    "body": [
      "//> using file ${1:FileName}.sc",
      "",
      "// brichka: exclude",
      "import ${1:FileName}._",
      "import org.apache.spark.sql.functions._",
      "// brichka: include"
    ],
    "description": "Brichka notebook import"
  },
  "brichka_notebook_new": {
    "prefix": "brnn",
    "body": [
      "//> using scala 2.13",
      "//> using dep com.nikolaiser::brichka-helpers::0.1.0",
      "//> using dep org.apache.spark::spark-sql::3.5.7",
      "//> using options \"-Yimports:java.lang,scala,scala.Predef,dev.DatabricksPredef\"",
      "",
      "// brichka: exclude",
      "import org.apache.spark.sql.functions._",
      "// brichka: include"
    ],
    "description": "Brichka new notebook"
  }
}
